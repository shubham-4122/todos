{
  "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
  "rules": {},
  "plugins": ["import", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["src", "./src"],
          ["actions", "./src/actions"],
          ["lib", "./src/lib"],
          ["store", "./src/store"],
          ["utils", "./src/utils"]
        ]
      }
    }
  }
}
